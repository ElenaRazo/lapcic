
@{
    ViewBag.Title = "Resultado";
    Layout = null;
}
<style type="text/css">
    body {
        font-size: 13px;
    }

    .nota {
        position: fixed;
        bottom: 0px;
        padding: 10px;
    }

    .ClaveHT {
        text-align: left;
        color: #DDD;
        font-size: 2em;
    }

    .HRCVE1 {
        position: absolute;
        top: 930px;
        text-align: right;
        z-index: -1;
    }

    .HRCVE2 {
        position: absolute;
        top: 2200px;
        text-align: right;
        z-index: -1;
    }

    .HRCVE3 {
        position: absolute;
        top: 3470px;
        text-align: right;
        z-index: -1;
    }

    .HRCVE4 {
        position: absolute;
        top: 4060px;
        text-align: right;
        z-index: -1;
    }
</style>

<table border="0" style="margin-top:0px;">
    <tbody>
        <tr >
            <td colspan="2">
                <div id="pnlEncabezado" style="width:100%">

                    <table cellspacing="0" border="0" id="FormView1" style="border-collapse:collapse;">
                        <tbody>
                            <tr>
                                <td colspan="2">
                                    <table border="0" cellpadding="0" cellspacing="0">
                                        <tbody>
                                            <tr>
                                                <td>
                                                    <img src="~/assets/demo/default/media/img/logo/logo.png" style="width:40%" />
                                                </td>
                                                <td align="right">
                                                    <strong>Clave: </strong>
                                                    <span id="FormView1_Label5">@ViewBag.Cotizacion.ClaveCotizacion</span>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="2">
                                                    <table style="border-bottom: black 1px solid">
                                                        <tbody>
                                                            <tr>
                                                                <td align="right" style="font-weight: bold; font-size: 13px; width: 69px;">
                                                                    Paciente:
                                                                </td>
                                                                <td style="width: 300px">
                                                                    <span id="FormView1_PACIENTELabel" style="font-size: 13px; ">@(ViewBag.Cotizacion.Nombre + " " + ViewBag.Cotizacion.Paterno + " "+ ViewBag.Cotizacion.Materno)</span>
                                                                </td>
                                                                <td align="right" style="font-weight: bold;font-size: 13px; ">
                                                                    Usuario:
                                                                </td>
                                                                <td style="width: 284px">
                                                                    <span id="FormView1_EMPRESALabel" style="font-size: 13px; ">@(ViewBag.Cotizacion.Usuario.Nombre + " " + ViewBag.Cotizacion.Usuario.Paterno + " " + ViewBag.Cotizacion.Usuario.Materno)</span>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td align="right" style="font-weight: bold; font-size: 13px; width: 69px;">
                                                                    Fecha de cotización:
                                                                </td>
                                                                <td style="width: 300px">
                                                                    <span id="FormView1_PACIENTELabel" style="font-size: 13px; ">@(ViewBag.Cotizacion.Fecha.ToString("dd/MM/yyyy HH:mm:ss"))</span>
                                                                </td>
                                                                <td align="right" style="font-weight: bold;font-size: 13px; ">
                                                                    Teléfono:
                                                                </td>
                                                                <td style="width: 284px">@ViewBag.Cotizacion.Laboratorio.Telefono
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <br />
                                    <center id="FormView1_LABORATORIOLabel" style="font-size: 11px">@ViewBag.Cotizacion.Laboratorio.Nombre</center>
                                    <br />
                                </td>
                            </tr>
                            <tr style="height:230px; vertical-align:top">
                               
                                <td style="width:50%">
                                    <table class="table table-striped m-table" style="width:100%;">
                                        <thead>
                                            <tr>
                                                <th style="width:70%;text-align:left;font-size: 13px;"> Nombre </th>
                                                <th style="width:30%;font-size: 13px;"> Precio </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{
                                                var total = 0.0;
                                                for (int i = 0; i < @ViewBag.Cotizacion.Estudios.Count; i++)
                                                {

                                                    total = total + ViewBag.Cotizacion.Estudios[i].PrecioBase;
                                                    <tr>
                                                        <td style="width:40%;font-size: 13px;">@ViewBag.Cotizacion.Estudios[i].Nombre</td>
                                                        <td style="width:30%; text-align:right;font-size: 13px;">@ViewBag.Cotizacion.Estudios[i].PrecioBase</td>
                                                    </tr>
                                                    if (i == ViewBag.Cotizacion.Estudios.Count - 1)
                                                    {
                                                        <tr>
                                                            <td style="width:40%;font-size: 13px;text-align:right;">TOTAL</td>
                                                            <td style="width:30%; text-align:right;font-size: 13px;">@total</td>
                                                        </tr>
                                                    }
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </td>
                                <td style="width:50%"></td>
                            </tr>
                        </tbody>
                    </table>

                </div>
            </td>
        </tr>
    </tbody>
</table>
<script language="javascript" type="text/javascript">
            //if (window.print())
              //  window.close();
</script>
